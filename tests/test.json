{
  "mapFileFormatVersion": 1,
  "metadata": {
    "defaultRecoveryRegister": "/IDN",
    "delimiter": "\r\n"
  },
  "registers": {
    "/cwFrequency": {
      "writeCmd": "SOUR:FREQ:CW {{x.0}}",
      "readCmd": "SOUR:FREQ:CW?",
      "readResp": "{{x.0}}\r\n",
      "type": "INT64"
    },
    "/cwFrequencyRO": {
      "readCmd": "SOUR:FREQ:CW?",
      "readResp": "{{x.0}}\r\n",
      "type": "INT64"
    },
    "/ACC": {
      "writeCmd": "ACC AXIS_1 {{x.0}} AXIS_2 {{x.1}} AXIS_3 {{x.2}} AXIS_4 {{x.3}} ACC AXIS_5 {{x.4}}",
      "readCmd": "ACC?",
      "readResp": "AXIS_1={{x.0}}\r\nAXIS_2={{x.1}}\r\nAXIS_3={{x.2}}\r\nAXIS_4={{x.3}}\r\nAXIS_5={{x.4}}\r\n",
      "nElem": 5,
      "nLnRead": 5,
      "type": "DOUBLE"
    },
    "/ACCRO": {
      "readCmd": "ACC?",
      "readResp": "AXIS_1={{x.0}}\r\nAXIS_2={{x.1}}\r\nAXIS_3={{x.2}}\r\nAXIS_4={{x.3}}\r\nAXIS_5={{x.4}}\r\n",
      "nElem": 5,
      "nLnRead": 5,
      "type": "DOUBLE"
    },
    "/SAI": {
      "readCmd": "SAI?",
      "readResp": "{{x.0}}\r\n{{x.1}}\r\n",
      "nElem": 2,
      "nLnRead": 2,
      "type": "STRING"
    },
    "/myData": {
      "readCmd": "CALC1:DATA:TRAC? 'myTrace' SDAT",
      "readResp": "{% for val in x %}{{val}}{% if not loop.is_last %},{% endif %}{% endfor %}\r\n",
      "nElem": 10,
      "nLnRead": 1,
      "type": "DOUBLE"
    },
    "/IDN": {
      "readCmd": "*IDN?",
      "readResp": "(.*)\r\n",
      "nElem": 1,
      "nLnRead": 1,
      "type": "STRING"
    },
    "/emergencyStopMovement": {
      "writeCmd": "\u0018",
      "type": "VOID"
    },
    "/ACC1": {
      "writeCmd": "ACC 1 {{x.0}}",
      "readCmd": "ACC?",
      "readResp": "1={{x.0}}\n\r\n",
      "nElem": 1,
      "nLnRead": 1,
      "type": "DOUBLE"
    },
    "/myHex": {
      "writeCmd": "HEX 0x{{x.0}} 0x{{x.1}} {{x.2}}",
      "readCmd": "HEX?",
      "readResp": "0x{{x.0}}\r\n0x{{x.1}}\r\n{{x.2}}\r\n",
      "nElem": 3,
      "nLnRead": 3,
      "type": "HEX"
    }
  }
}
